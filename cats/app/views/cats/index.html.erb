<table>
    <thead>
        <tr>
            <th colspan="3">99 Cats</th>
        </tr
    </thead>
    <tbody>
        <tr>
            <% @cats.each do |cat| %>
            <td>
                <ul>
                    <%= render 'cat', cat: cat, action: :index %>
                </ul>
            </td>
            <% end %>
        </tr>
    </tbody>
</table>




